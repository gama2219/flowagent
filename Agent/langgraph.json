{
  "$schema": "https://langgra.ph/schema.json",
  "dependencies": ["."],
  "graphs": {
    "agent": "./src/agent/graph.py:graph"
  },
  "env": ".env",
  "auth": {
  	"path": "./auth/auth.py:auth"
  },
  "http": {
  	"app": "./src/agent/webapp.py:app"
  },
  "checkpointer": {
  	"ttl": {
  		"strategy": "delete",
  		"sweep_interval_minutes": 60,
  		"default_ttl": 43200
  	}
  },
  "dockerfile_lines": [
    "RUN python v_db.py"
  ]
}
